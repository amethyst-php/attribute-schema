{
    "name": "amethyst/attribute-schema",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-attribute-schema", "attribute-schema"],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "amethyst/core": "0.3.*",
        "amethyst/relation-schema": "0.3.*",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "amethyst/foo": "0.3.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "attribute-schema",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\AttributeSchemaServiceProvider"
            ]
        }
    }
}
